/*
Bubble Sort Algorithm
Approach:
Time Complexity: O(n^2)
Space Complexity 
*/

public void bubbleSort(int[] arr)
{
  int n = arr.length;
  for(int i=0;i<n-1;i++){
    for(int j= 0;j<n-1-i;j++){
      if(arr[i]>arr[j+1]){
        swap(A,i,j+1);
      }
     }
   }
}

public void swap(int[] arr, int i, int j)
{
  int temp = arr[i];
  arr[i] = arr[j];
  arr[j] = temp;
}
